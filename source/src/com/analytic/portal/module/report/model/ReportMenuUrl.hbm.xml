<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.analytic.portal.module.report.model.ReportMenuUrl" table="report_menu_url">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="userId" type="java.lang.String">
            <column name="user_id" />
        </property>
        <property name="menuId" type="java.lang.String">
            <column name="menu_id" />
        </property>
        <property name="reportUrl" type="java.lang.String">
            <column name="report_url" length="100" />
        </property>
    </class>
</hibernate-mapping>
