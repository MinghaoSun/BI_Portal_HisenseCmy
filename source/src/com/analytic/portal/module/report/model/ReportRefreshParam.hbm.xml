<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.analytic.portal.module.report.model.ReportRefreshParam" table="report_param">
        <id name="id" type="java.lang.String">
            <column name="id" length="100"/>
            <generator class="assigned" />
        </id>
        <property name="code" type="java.lang.String">
            <column name="code" length="100" />
        </property>
        <property name="paramName" type="java.lang.String">
            <column name="param_name" length="100" />
        </property>
        <property name="paramKey" type="java.lang.String">
            <column name="param_key"  length="100" />
        </property>
        <property name="paramOrder" type="java.lang.Integer">
            <column name="param_order" />
        </property>
        <property name="paramRemark" type="java.lang.String">
            <column name="param_remark" length="200" />
        </property>
		<property name="isDelete" type="java.lang.String">
            <column name="is_delete"  length="1"/>
        </property>
        <property name="createTime" type="java.lang.String" update="false">
            <column name="create_time" length="100" />
        </property>
        <property name="createUser" type="java.lang.String">
            <column name="create_user" length="100" />
        </property>
        <property name="updateTime" type="java.lang.String">
            <column name="update_time" length="100" />
        </property>
        <property name="updateUser" type="java.lang.String">
            <column name="update_user" length="19"/>
        </property>
        <property name="relateDataType" type="java.lang.String">
            <column name="relate_data_type" length="2"/>
        </property>
        <property name="paramValue" type="java.lang.String">
            <column name="param_value" length="100"/>
        </property>
        <property name="paramOperatType" type="java.lang.String">
            <column name="param_operat_type" length="2"/>
        </property>
        <property name="paramValueType" type="java.lang.String">
            <column name="param_value_type" length="2"/>
        </property>
        <property name="paramValueRelate" type="java.lang.String">
            <column name="param_value_relate" length="100"/>
        </property>
        <property name="refreshValue" type="java.lang.String">
            <column name="refresh_value" length="255"/>
        </property>
        <property name="paramType" type="java.lang.String">
            <column name="param_type" length="1"/>
        </property>
    </class>
</hibernate-mapping>
