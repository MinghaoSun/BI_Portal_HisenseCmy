<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.analytic.portal.module.system.model.SysRole" table="sys_role">
        <id name="id" type="java.lang.String">
            <column name="id"/>
            <generator class="assigned" />
        </id>
        <property name="code" type="java.lang.String">
            <column name="code" length="100" />
        </property>
        <property name="roleName" type="java.lang.String">
            <column name="role_name" length="100" />
        </property>
        <property name="roleNameEn" type="java.lang.String">
            <column name="role_name_en" length="100" />
        </property>
        <property name="roleNameZh" type="java.lang.String">
            <column name="role_name_zh" length="100" />
        </property>
        <property name="roleStatus" type="java.lang.String">
            <column name="role_status" />
        </property>
        <property name="subRoleId" type="java.lang.String">
            <column name="sub_role_id" />
        </property>
        <property name="roleType" type="java.lang.String">
            <column name="role_type" length="1"/>
        </property>
        <property name="roleRemark" type="java.lang.String">
            <column name="role_remark" length="200" />
        </property>
        <property name="createTime" type="java.lang.String" update="false">
            <column name="create_time" length="100" />
        </property>
        <property name="createUser" type="java.lang.String">
            <column name="create_user" length="100" />
        </property>
        <property name="updateTime" type="java.lang.String">
            <column name="update_time" length="100" />
        </property>
        <property name="updateUser" type="java.lang.String">
            <column name="update_user" length="100" />
        </property>
        <property name="isDelete" type="java.lang.String">
            <column name="is_delete"  length="1"/>
        </property>
    </class>
</hibernate-mapping>
