<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.analytic.portal.module.system.model.SysUser" table="sys_user">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="loginName" type="java.lang.String">
            <column name="login_name" length="100" />
        </property>
        <property name="loginPassword" type="java.lang.String">
            <column name="login_password" length="100" />
        </property>
        <property name="userFullName" type="java.lang.String">
            <column name="user_full_name" length="100" />
        </property>
        <property name="firstName" type="java.lang.String">
            <column name="first_name" length="100" />
        </property>
        <property name="lastName" type="java.lang.String">
            <column name="last_name" length="100" />
        </property>
        <property name="userSex" type="java.lang.String">
            <column name="user_sex" length="1" />
        </property>
        <property name="userBirthday" type="java.lang.String">
            <column name="user_birthday" length="100" />
        </property>
        <property name="userMobile" type="java.lang.String">
            <column name="user_mobile" length="100" />
        </property>
        <property name="userEmail" type="java.lang.String">
            <column name="user_email" length="100" />
        </property>
        <property name="isOnline" type="java.lang.String">
            <column name="is_online" length="1" />
        </property>
        <property name="userEnabled" type="java.lang.String">
            <column name="user_enabled" length="1" />
        </property>
        <property name="isDelete" type="java.lang.String">
            <column name="is_delete" length="1" />
        </property>
        <property name="lastLoginTime" type="java.lang.String">
            <column name="last_login_time" length="19" />
        </property>
        <property name="dataRoleId" type="java.lang.String">
            <column name="data_role_id" length="200" />
        </property>
         <property name="userThemes" type="java.lang.String">
            <column name="user_themes" length="100" />
        </property>
        <property name="userRemark" type="java.lang.String">
            <column name="user_remark" length="200" />
        </property>
        <property name="createTime" type="java.lang.String" update="false">
            <column name="create_time"  length="100" />
        </property>
        <property name="createUser" type="java.lang.String">
            <column name="create_user" length="100" />
        </property>
        <property name="updateTime" type="java.lang.String" >
            <column name="update_time"  length="100" />
        </property>
        <property name="updateUser" type="java.lang.String">
            <column name="update_user" length="100" />
        </property>
        <property name="userDepartment" type="java.lang.String">
            <column name="user_department" length="100" />
        </property>
        <property name="userFlag" type="java.lang.String">
            <column name="user_flag" length="100" />
        </property>
    </class>
</hibernate-mapping>
