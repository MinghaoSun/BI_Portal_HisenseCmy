<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.analytic.portal.module.system.model.SysUserReportSetting" table="sys_user_report_setting">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="userId" type="java.lang.String">
            <column name="user_id" length="100" />
        </property>
        <property name="menuId" type="java.lang.String">
            <column name="menu_id" length="100" />
        </property>
        <property name="settingType" type="java.lang.String">
            <column name="setting_type" length="10" />
        </property>
        <property name="deletedFlag" type="java.lang.String">
            <column name="deleted_flag" length="1" />
        </property>
    </class>
</hibernate-mapping>
