<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.analytic.portal.module.system.model.SysConfig" table="sys_config">
        <id name="id" type="java.lang.String">
            <column name="id"/>
            <generator class="assigned" />
        </id>
        <property name="paramName" type="java.lang.String">
            <column name="param_name" length="100" />
        </property>
        <property name="paramCode" type="java.lang.String">
            <column name="param_code" length="100" />
        </property>
        <property name="paramValue" type="java.lang.String">
            <column name="param_value" length="100" />
        </property>
        <property name="paramType" type="java.lang.String">
            <column name="param_type" length="100" />
        </property>
        <property name="paramDesc" type="java.lang.String">
            <column name="param_desc" />
        </property>
    </class>
</hibernate-mapping>
