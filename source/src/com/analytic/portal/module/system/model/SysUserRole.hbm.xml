<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.analytic.portal.module.system.model.SysUserRole" table="sys_user_role" >
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="userId" type="java.lang.String">
            <column name="user_id" />
        </property>
        <property name="roleId" type="java.lang.String">
            <column name="role_id" />
        </property>
        <property name="createTime" type="java.lang.String">
            <column name="create_time" length="100" />
        </property>
        <property name="createUser" type="java.lang.String">
            <column name="create_user" length="100" />
        </property>
        <property name="updateTime" type="java.lang.String">
            <column name="update_time" length="100" />
        </property>
        <property name="updateUser" type="java.lang.String">
            <column name="update_user" length="100" />
        </property>
    </class>
</hibernate-mapping>
