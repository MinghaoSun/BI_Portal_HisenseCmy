<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.analytic.portal.module.system.model.SysRoleReport" table="sys_role_report">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="roleId" type="java.lang.String">
            <column name="role_id" />
        </property>
        <property name="menuId" type="java.lang.String">
            <column name="menu_id"  />
        </property>
        <property name="reportToolId" type="java.lang.String">
            <column name="report_tool_id" />
        </property>
    </class>
</hibernate-mapping>